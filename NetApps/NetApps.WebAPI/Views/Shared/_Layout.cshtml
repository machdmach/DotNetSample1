@{
    var vData = MyViewData.Prep(this, true);
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@vData.HeadTitle</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/site.css" />

    @Html.Raw(vData.GetHtmlHeadExtraHtml())
</head>
<body>
    <header>
        @Html.Raw(vData.GetHeaderhtml())
    </header>

    @RenderBody()

    <footer>
    </footer>

    @Html.Raw(vData.FooterDebugInfoBuf.ToString())
</body>
</html>
